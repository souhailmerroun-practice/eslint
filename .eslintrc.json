{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        //https://eslint.org/docs/rules/semi
        "semi": [
            "error",
            "always"
        ],
        //https://eslint.org/docs/rules/quotes
        "quotes": [
            "error",
            "double"
        ]
    }
}